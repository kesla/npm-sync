{
  "name": "npm-sync",
  "version": "0.0.0",
  "description": "syncs a node_modules folder with an ideal npm package tree",
  "main": "dist/index.js",
  "bin": {
    "npm-sync": "dist/cli.js"
  },
  "scripts": {
    "test": "babel-tape-runner test.js && semistandard | snazzy",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build",
    "start": "babel-watch lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/npm-sync.git"
  },
  "author": "David Bj√∂rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesla/npm-sync/issues"
  },
  "homepage": "https://github.com/kesla/npm-sync#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-tape-runner": "^2.0.1",
    "babel-watch": "^2.0.2",
    "lodash.sortby": "^4.5.0",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0",
    "tapava": "^2.2.0",
    "then-fs": "^2.0.0",
    "tmp": "0.0.28"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "download-npm-package": "^3.1.1",
    "get-ideal-package-tree": "^1.0.3",
    "object.entries": "^1.0.3",
    "read-package-json": "^2.0.4"
  }
}
